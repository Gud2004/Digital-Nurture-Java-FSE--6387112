SET SERVEROUTPUT ON;

BEGIN
  FOR cust IN (
    SELECT CustomerID,
           InterestRate,
           Age
    FROM Customers
  ) LOOP
    IF cust.Age > 60 THEN
         UPDATE Customers
         SET InterestRate = cust.InterestRate - 1
        WHERE CustomerID = cust.CustomerID;

    END IF;
  END LOOP;

  COMMIT;
END;
